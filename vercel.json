{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/music/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/music/(.*)",
      "dest": "/public/music/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
